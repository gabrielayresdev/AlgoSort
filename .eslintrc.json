{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "class-methods-use-this": 0,
    "no-plusplus": 0,
    "import/extensions": 0,
    "no-await-in-loop": 0
  }
}
